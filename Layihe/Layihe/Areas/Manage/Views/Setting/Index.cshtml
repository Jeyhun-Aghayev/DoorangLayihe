@model List<Setting>
<div style="width:100%;margin:10px">
    <h1>Post Table</h1>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">No#</th>
                <th scope="col">Id</th>
                <th scope="col">Key</th>
                <th scope="col">Value</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @{
                int count = 0;
            }
            @foreach (var item in Model)
            {
                count++;
                <tr>
                    <th scope="row">@count</th>
                    <td>@item.Id</td>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                    <td>
                        <a class="btn btn-warning" asp-action="Update" asp-controller="Setting" asp-area="Manage" asp-route-id="@item.Id">Update</a>
                        <a class="btn btn-danger" onclick="return confirm('Are you sure you want to delete this item')" asp-action="Delete" asp-controller="Setting" asp-route-id="@item.Id" asp-area="Manage">Delete</a>
                    </td>
                </tr>
            }

        </tbody>
    </table>
</div>